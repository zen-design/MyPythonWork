   "source": [
    "import random\n",
    "\n",
    "NUMBER_OF_TRIALS = 1000000\n",
    "numberOfHits = 0\n",
    "\n",
    "for i in range(NUMBER_OF_TRIALS):\n",
    "    x = random.random() * 2 - 1\n",
    "    y = random.random() * 2 - 1\n",
    "    \n",
    "    if x * x + y * y <= 1:\n",
    "        numberOfHits += 1\n",
    "        \n",
    "pi = 4 * numberOfHits / NUMBER_OF_TRIALS\n",
    "print(\"PI is\", pi)"
   ]
